<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         id="WebApp_ID" version="3.1">


    <servlet>
        <servlet-name>WebTest</servlet-name>
        <servlet-class>javaee.controller.WebTest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WebTest</servlet-name>
        <url-pattern>/showall</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HelloWorldController</servlet-name>
        <servlet-class>javaee.controller.HelloWorldController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloWorldController</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>
</web-app>
